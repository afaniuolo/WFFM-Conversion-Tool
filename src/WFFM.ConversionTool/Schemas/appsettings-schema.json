{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-03/schema#",
    "type": "object",
    "title": "The AppSettings file Schema",
    "properties": {
        "itemReferences": {
            "$id": "#/properties/itemReferences",
            "type": "object",
            "required": true,
            "title": "The Itemreferences Schema",
            "properties": {

            }
        },
        "converters": {
            "$id": "#/properties/converters",
            "type": "array",
            "required": true,
            "title": "The Converters Schema",
            "items": {
                "$id": "#/properties/converters/items",
                "type": "object",
                "title": "The Items Schema",
                "properties": {
                    "name": {
                        "$id": "#/properties/converters/items/properties/name",
                        "type": "string",
                        "required": true,
                        "title": "The Name Schema",
                        "default": "",
                        "examples": [
                            "TrackingConverter"
                        ]
                    },
                    "converterType": {
                        "$id": "#/properties/converters/items/properties/converterType",
                        "type": "string",
                        "required": true,
                        "title": "The Convertertype Schema",
                        "default": "",
                        "examples": [
                            "WFFM.ConversionTool.Library.Converters.FieldConverters.TrackingConverter, WFFM.ConversionTool.Library"
                        ]
                    }
                }
            }
        },
        "submitActions": {
            "$id": "#/properties/submitActions",
            "type": "array",
            "required": true,
            "title": "The SubmitActions Schema",
            "items": {
                "$id": "#/properties/submitActions/items",
                "type": "object",
                "title": "The Items Schema",
                "properties": {
                    "sourceSaveActionId": {
                        "$id": "#/properties/submitActions/items/properties/sourceSaveActionId",
                        "type": "string",
                        "required": true,
                        "title": "The SourceSaveActionId Schema",
                        "default": "",
                        "examples": [
                            "{AD26FE98-EED1-45C8-95AE-F2714EE33C62}"
                        ]
                    },
                    "destSubmitActionFieldValue": {
                        "$id": "#/properties/submitActions/items/properties/destSubmitActionFieldValue",
                        "type": "string",
                        "required": true,
                        "title": "The DestSubmitActionFieldValue Schema",
                        "default": "",
                        "examples": [
                            "{4A937D74-7986-4E19-9D8E-EC14675B17F0}"
                        ]
                    },
                    "destParametersConverterType": {
                        "$id": "#/properties/submitActions/items/properties/destParametersConverterType",
                        "type": "string",
                        "required": true,
                        "title": "The DestParametersConverterType Schema",
                        "default": "",
                        "examples": [
                            "WFFM.ConversionTool.Library.Converters.SaveActionParametersConverters.TriggerCampaignParametersConverter, WFFM.ConversionTool.Library"
                        ]
                    }
                }
            }
        },
        "metadataFolderRelativePath": {
            "$id": "#/properties/metadataFolderRelativePath",
            "type": "string",
            "required": true,
            "title": "The Metadatafolderrelativepath Schema",
            "default": "",
            "examples": [
                "Metadata"
            ]
        },
        "invalidItemNameChars": {
            "$id": "#/properties/invalidItemNameChars",
            "type": "string",
            "required": true,
            "title": "The Invaliditemnamechars Schema",
            "default": "",
            "examples": [
                "/:?&quot;&lt;&gt;|[]"
            ]
        },
        "enableReferencedItemCheck": {
            "$id": "#/properties/enableReferencedItemCheck",
            "type": "boolean",
            "required": true,
            "title": "The Enablereferenceditemcheck Schema",
            "default": false,
            "examples": [
                false
            ]
        },
        "formsDataProvider": {
            "$id": "#/properties/formsDataProvider",
            "type": "string",
            "required": true,
            "title": "The Formsdataprovider Schema",
            "default": "",
            "examples": [
                "sqlFormsDataProvider"
            ]
        }
    }
}
